            <profile id="9239-e2cf-56b2-72f9::4c40-9224-1518-21c9::54df-f8eb-92f5-87fb" name="Pathfinder Bonding Knife Ritual" hidden="false" typeId="1015-f0b1-9137-0060" typeName="Ability">
