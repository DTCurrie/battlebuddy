                    <characteristic name="S" typeId="fcc6-35ea-38b6-f4ca">-</characteristic>
                    <characteristic name="AP" typeId="fc0e-2874-184d-9f64">-</characteristic>
                    <characteristic name="D" typeId="cc1f-e463-c014-2251">-</characteristic>
                    <characteristic name="Abilities" typeId="72cf-5b8f-5b71-79b2">See Markerlight rules.</characteristic>
                  </characteristics>
                </profile>
                <profile id="bb60-a729-694c-9fbd::9da7-39af-7eee-3b48::f386-80dc-e623-1f2d::2d10-c4cb-6ffa-9157" name="Target lock" hidden="false" typeId="0883-432d-b2ca-f11a" typeName="Wargear">
